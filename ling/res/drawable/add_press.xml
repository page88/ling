<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
	<item>
        <shape android:shape="oval">
            
            <solid android:color="@color/title_bg"/>
            
            <stroke 
                android:width="1dp"
                android:color="@color/title_bg"/>
            
            <size 
                android:width="50dp"
                android:height="50dp"/>
        </shape>
    </item>
    
    <item 
        android:left="10dp"
        android:top="22dp"
        android:right="10dp"
        android:bottom="22dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>      
        </shape>
    </item>        
    
    <item 
        android:left="22dp"
        android:top="10dp"
        android:right="22dp"
        android:bottom="10dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>      
        </shape>
    </item>    

</layer-list>
